{
  "name": "roguelike72",
  "version": "1.0.0",
  "description": "Probably my 72nd attempt at a roguelike.",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "src/index.ts",
  "targets": {
    "default": {
      "distDir": "./docs",
      "publicUrl": "/yet-another-roguelike-engine/"
    }
  },
  "scripts": {
    "start": "parcel src/index.html --open",
    "prebuild": "rimraf docs",
    "build": "parcel build src/index.html",
    "tsc": "tsc --noEmit",
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest --coverage",
    "circular": "npx dpdm src --circular",
    "coverage": "run coverage/lcov-report/index.html",
    "test:watch": "jest --watch"
  },
  "author": "Lag.Com <lagdotcom@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@lagdotcom/boring-parser": "github:lagdotcom/boring-parser",
    "debug": "^4.3.3",
    "deep-object-diff": "^1.1.7",
    "deepcopy": "^2.1.0",
    "eventemitter3": "^4.0.7",
    "lodash.merge": "^4.6.2",
    "nanoid": "^5.0.5",
    "parcel": "^2.3.2",
    "random-seedable": "^1.0.8",
    "wglt": "^0.6.4"
  },
  "devDependencies": {
    "@parcel/resolver-glob": "^2.3.2",
    "@parcel/transformer-inline-string": "^2.3.2",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.5.12",
    "@types/lodash.merge": "^4.6.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "buffer": "^6.0.3",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
